{
    "name": "eventsourcerer/symfony-client",
    "description": "A transport to be used with Symfony Messenger to retrieve events from EventSourcerer",
    "type": "library",
    "autoload": {
        "psr-4": {
            "EventSourcerer\\ClientBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EventSourcerer\\ClientBundle\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "michaelbudd",
            "email": "michaelbudd6@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^12.0",
        "symfony/http-kernel": "7.3.*"
    },
    "require": {
        "php": ">=8.4",
        "symfony/dependency-injection": "7.3.*",
        "symfony/messenger": "7.3.*",
        "symfony/http-client": "7.3.*",
        "eventsourcerer/event-sourcerer-client": "dev-main"
    },
    "minimum-stability": "dev"
}
